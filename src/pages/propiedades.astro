---
import Layout from "../layouts/Layout.astro";
import PropiedadesAlquilerApp from "../components/PropiedadesAlquilerApp.tsx";
---

<Layout title="Argensador | Propiedades en Alquiler">
  <div class="flex flex-col max-w-6xl mx-auto px-4 py-8 gap-4">
    <div class="flex flex-col max-w-6xl mx-auto px-4 py-8 gap-4">
      <h1 class="text-4xl font-bold text-center text-rosaOscuro relative">
        Propiedades en Alquiler
      </h1>

      <section>
        <PropiedadesAlquilerApp client:load />
      </section>
    </div>
  </div>
</Layout>
