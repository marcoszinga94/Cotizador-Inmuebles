{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "node16",
    "module": "Node16",
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "useUnknownInCatchVariables": false,
    "types": [
      "node",
      "firebase"
    ]
  }
}